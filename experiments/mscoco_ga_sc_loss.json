    "input_label_h5": "data/cocotalk_label.h5",
    "use_att": false,
    "use_box": false,
    "norm_att_feat": 0,
    "norm_box_feat": 0,
    "input_box_dir": "",

    "seq_per_img": 5,
    "train_only": 1
  },

  "caption_model_options": {
    "fitness": "sc_loss",

    "input_encoding_size":  128,
    "rnn_type": "lstm",
    "rnn_size": 128,
    "num_layers": 1,
    "drop_prob_lm": 0.0,
    "fc_feat_size": 2048

  },
  "_seq_length":  0,
  "_vocab_size":  0,

  "parents_dir": "parents",
  "offspring_dir": "offspring",
  "elite_dir": "elite",
  "best_dir": "best"
