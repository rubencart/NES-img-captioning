{
  "mode":  "nets",
  "config": {
    "cuda": false,

    "eval_prob": 0.004,
    "noise_stdev": 0.002,
    "snapshot_freq": 5,
    "batch_size": 256,

    "val_batch_size": 256,
    "num_val_batches": 4,
    "num_val_items": 5000,

    "patience": 30,
    "stdev_decr_divisor": 0.9
  },

  "dataset": "mscoco",
  "net": "fc_caption",
  "population_size": 1000,
  "truncation": 30,

  "num_elites": 3,
  "num_elite_cands": 3,

  "from_single": "logs/log_xent_fc_128/checkpt/model-best.pth",

  "caption_options": {
    "input_json": "data/cocotalk.json",
    "input_fc_dir": "data/cocobu_fc",
